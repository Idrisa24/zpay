{
    "name": "saidtech/zpay",
    "description": "This package is for making an online payments through mpesa and azam pay",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "barryvdh/laravel-dompdf": "2.0.x-dev",
        "simplesoftwareio/simple-qrcode": "dev-develop",
        "phpseclib/phpseclib": "~3.0"
    },
    "keywords": [
        "payment",
        "azam pay",
        "azam",
        "vodacom",
        "mpesa",
        "zpay"
    ],
    "license": "MIT",
    "autoload": {
